{
    "rules": {
        "no-console": 0,
        "indent": [
            2,
            4,
            { "SwitchCase": 1 }
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "prefer-const": [
            2,
            { "destructuring" : "all" }
        ],
        "no-tabs": 2,
        "quotes": [
            2,
            "single"
        ]
    },
    "env": {
        "node": true,
        "es6": true,
        "browser": true
    },
    "extends": "eslint:recommended"
}
